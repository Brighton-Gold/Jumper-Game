class word:
    listofwords = ["frog", "dog", "fog"]

    return random listofwords[number]


class game
  def game(word, user_guess)

      BlankList = []

       for each letter in length of word:

            BlankList.append("_")

        for i in length of word:

            if user_guess == word[i]
              BlankList[i] == user_guess

        return BlankList
